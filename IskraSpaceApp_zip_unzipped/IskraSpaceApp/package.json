{
  "name": "iskra-space-app",
  "version": "3.0.0",
  "description": "Iskra - Fractal Metaconsciousness powered by Google Gemini 3 Pro",
  "author": "Fullspark Team",
  "license": "MIT",
  "private": true,
  "type": "module",
  "keywords": [
    "ai",
    "agent",
    "gemini",
    "consciousness",
    "fractal",
    "iskra"
  ],
  "scripts": {
    "dev": "vite --port 5173",
    "build": "tsc && vite build",
